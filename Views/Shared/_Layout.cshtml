<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - shredhousepage</title>
    <meta charset="utf-8">
    <title>Shredded Packaging | shredded paper | shredded tissue | UK Manufacturers</title>
    <meta name="description"
          content="UK manufacturers of shredded products in paper, tissue, cellophane, iridescent film">
    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@@ShredhouseLtd">
    <meta name="twitter:title" content="Shredded Packaging | shredded paper | shredded tissue | UK Manufacturers">
    <meta name="twitter:description"
          content="UK manufacturers of shredded products in paper, tissue, cellophane, iridescent film">
    <meta name="twitter:creator" content="@@ShredhouseLtd">
    <!-- Twitter Summary card images must be at least 120x120px -->
    <meta name="twitter:image" content="https://www.shredhouse.co.uk/shredhouse-og-image.jpg">
    <!-- Open Graph data -->
    <meta property="og:title" content="Shredded Packaging | shredded paper | shredded tissue | UK Manufacturers" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="https://www.shredhouse.co.uk" />
    <meta property="og:image" content="https://www.shredhouse.co.uk/shredhouse-og-image.jpg" />
    <meta property="og:description"
          content="UK manufacturers of shredded products in paper, tissue, cellophane, iridescent film" />
    <meta property="og:site_name" content="Shredhouse" />
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!--Mobileness-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1, user-scalable=0">
    <meta name="format-detection" content="telephone=no">
    <!-- Stylesheets-->

    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
    <!--[if IE]>
      <script src="https://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <!-- Contact form -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.microsoft.com/ajax/jquery.validate/1.7/jquery.validate.pack.js"type="text/javascript"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</head>
<body>
